<Window x:Class="ObjectEditor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ObjectEditor"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Name="ObjectProperty">
            <Label>CameraPosition</Label>
            <TextBlock Name="CameraPosition"></TextBlock>
            <Button Name="CreateObjectBtn">Create Sphere</Button>
        </StackPanel>
    </Grid>
</Window>
